<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="kz.inqbox.sdk.sample">

    <application
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:name=".SampleApplication"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.InQBox">

        <!-- Entry Point -->
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <!-- socket -->
        <activity android:name=".samples.socket.SocketActivity" />

        <!-- webrtc -->
        <activity android:name=".samples.webrtc.WebRTCActivity" />
        <activity android:name=".samples.webrtc.PreviewActivity" />

    </application>

</manifest>